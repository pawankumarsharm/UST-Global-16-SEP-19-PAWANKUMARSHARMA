package com.ustglobal.jpawithhibernateonetoone;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Entity
@Table(name="voter")
public class Voter {
	@Id
@Column
	private int vid;
@Column
private String vname;

}
